{
  "$schema": "https://schema.freqtrade.io/schema.json",
  "max_open_trades": 10,
  "stake_currency": "USDT",
  "stake_amount": 10000,
  "tradable_balance_ratio": 0.99,
  "fiat_display_currency": "USD",
  "amount_reserve_percent": 0.05,

  "dry_run": true,
  "dry_run_wallet": 100000,

  "cancel_open_orders_on_exit": false,
  "timeframe": "5m",

  // 不使用Trailing stop来简化，若想尝试可以设true
  "trailing_stop": false,
  "trailing_stop_positive": 0.005,
  "trailing_stop_positive_offset": 0.0051,
  "trailing_only_offset_is_reached": false,

  "use_exit_signal": true,
  "exit_profit_only": false,
  "exit_profit_offset": 0.0,
  "ignore_roi_if_entry_signal": false,
  "ignore_buying_expired_candle_after": 300,

  "trading_mode": "spot",
  "margin_mode": "",

  // （可根据需求微调）
  "minimal_roi": {
    "60": 0.01, // 持有1小时后，满足1%盈利就可卖出
    "30": 0.02,
    "0": 0.03 // 30分钟内若涨到3%就卖
  },

  // 维持 -10% 止损，后续可加自定义止损
  "stoploss": -0.1,

  "unfilledtimeout": {
    "entry": 30, // 给限价单 30 分钟等待成交
    "exit": 10,
    "exit_timeout_count": 0,
    "unit": "minutes"
  },

  "entry_pricing": {
    "price_side": "same",
    "use_order_book": true,
    "order_book_top": 1,
    "price_last_balance": 0.0,
    "check_depth_of_market": {
      "enabled": false,
      "bids_to_ask_delta": 1
    }
  },
  "exit_pricing": {
    "price_side": "same",
    "use_order_book": true,
    "order_book_top": 1,
    "price_last_balance": 0.0
  },
  "order_types": {
    "entry": "limit", // 仍使用限价单
    "exit": "limit",
    "emergency_exit": "market",
    "force_exit": "market",
    "force_entry": "market",
    "stoploss": "market",
    "stoploss_on_exchange": false, // 让freqtrade本地监控Stoploss
    "stoploss_price_type": "last",
    "stoploss_on_exchange_interval": 60,
    "stoploss_on_exchange_limit_ratio": 0.99
  },
  "order_time_in_force": {
    "entry": "GTC",
    "exit": "GTC"
  },

  "pairlists": [
    {
      "method": "StaticPairList"
    },
    {
      "method": "FullTradesFilter"
    },
    // 这里可以把 "VolumePairList"注释掉或改小"number_assets" 以减少过多筛选
    // 目前保留但可放宽
    {
      "method": "VolumePairList",
      "number_assets": 20,
      "sort_key": "quoteVolume",
      "refresh_period": 1800
    },
    {
      "method": "AgeFilter",
      "min_days_listed": 10
    },
    {
      "method": "PrecisionFilter"
    },
    {
      "method": "PriceFilter",
      "low_price_ratio": 0.01,
      "min_price": 0.0000001
    },
    // SpreadFilter 改为 1% (0.01) 或直接删除
    {
      "method": "SpreadFilter",
      "max_spread_ratio": 0.01
    },
    // 如果想简化，可以注释此Filter
    {
      "method": "RangeStabilityFilter",
      "lookback_days": 10,
      "min_rate_of_change": 0.01,
      "refresh_period": 1440
    }
  ],

  "exchange": {
    "name": "binanceus",
    "key": "your_exchange_key",
    "secret": "your_exchange_secret",
    "password": "",
    "log_responses": false,
    "ccxt_config": {
      "timeout": 30000,
      "enableRateLimit": true,
      "retry": {
        "maxRetries": 3,
        "delay": 5000
      }
    },
    "ccxt_async_config": {
      "rateLimit": 1000,
      "timeout": 30000
    },
    "pair_whitelist": [
      "BTC/USDT",
      "ETH/USDT",
      "SOL/USDT",
      "ADA/USDT",
      "XRP/USDT",
      "DOT/USDT",
      "MATIC/USDT",
      "LTC/USDT",
      "SHIB/USDT",
      "AVAX/USDT",
      "LINK/USDT",
      "ATOM/USDT"
    ],
    "pair_blacklist": ["DOGE/USDT"],
    "outdated_offset": 5,
    "markets_refresh_interval": 60,
    "enable_ws": false
  },

  "api_server": {
    "enabled": true,
    "listen_ip_address": "0.0.0.0",
    "listen_port": 8082,
    "verbosity": "error",
    "enable_openapi": false,
    "jwt_secret_key": "somethingrandom",
    "CORS_origins": [],
    "username": "freqtrader",
    "password": "SuperSecurePassword",
    "ws_token": "secret_ws_t0ken."
  },
  "external_message_consumer": {
    "enabled": false,
    "producers": [],
    "wait_timeout": 300,
    "ping_timeout": 10,
    "sleep_time": 10,
    "remove_entry_exit_signals": false,
    "message_size_limit": 8
  },

  "internals": {
    "process_throttle_secs": 5,
    "heartbeat_interval": 60
  },

  "bot_name": "freqtrade3",
  "db_url": "sqlite:///tradesv3.freqtrade3.sqlite",
  "initial_state": "running",
  // 注意这里使用下面的策略名字
  "strategy": "SampleStrategyEnhanced",
  "strategy_path": "user_data/strategies/"
}
